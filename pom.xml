    <build>
        <sourceDirectory>src/main/java</sourceDirectory>
        <testSourceDirectory>src/test/java</testSourceDirectory>

        <finalName>${project.artifactId}</finalName>
        <resources>
            <resource>
                <directory>src/main/resources/</directory>
                <filtering>true</filtering>
                <includes>
                    <include>plugin.yml</include>
                    <include>version</include>
                    <include>META-INF/*</include>
                    <include>config.sk</include>
                    <include>lang/*</include>
                    <include>scripts/configuration example.sk</include>
                </includes>
            </resource>
            <resource>
                <directory>src/main/resources/</directory>
                <filtering>false</filtering>
                <includes>
                    <include>**/*</include>
                </includes>
                <excludes>
                    <exclude>plugin.yml</exclude>
                    <exclude>version</exclude>
                    <exclude>META-INF/*</exclude>
                    <exclude>config.sk</exclude>
                    <exclude>lang/*</exclude>
                    <exclude>scripts/configuration example.sk</exclude>
                </excludes>
            </resource>
            <resource>
                <directory>./</directory>
                <filtering>false</filtering>
                <includes>
                    <include>LICENSE</include>
                    <include>README.md</include>
                </includes>
            </resource>
        </resources>

        <testResources>
            <testResource>
                <directory>src/test/resources/</directory>
                <filtering>false</filtering>
            </testResource>
        </testResources>

        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-enforcer-plugin</artifactId>
                <version>3.0.0-M3</version>
                <dependencies>
                    <dependency>
                        <groupId>org.codehaus.mojo</groupId>
                        <artifactId>extra-enforcer-rules</artifactId>
                        <version>1.3</version>
                        <exclusions>
                            <exclusion>
                                <groupId>org.codehaus.plexus</groupId>
                                <artifactId>plexus-utils</artifactId>
                            </exclusion>
                        </exclusions>
                    </dependency>
                    <dependency>
                        <groupId>org.codehaus.plexus</groupId>
                        <artifactId>plexus-utils</artifactId>
                        <version>3.3.0</version>
                    </dependency>
                </dependencies>
                <executions>
                    <execution>
                        <id>enforce-maven</id>
                        <phase>validate</phase>
                        <goals>
                            <goal>enforce</goal>
                        </goals>
                        <configuration>
                            <rules>
                                <requireMavenVersion>
                                    <version>3.0.5</version>
                                </requireMavenVersion>
                                <banDuplicatePomDependencyVersions/>
                                <banDuplicateClasses>
                                    <scopes>
                                        <scope>compile</scope>
                                        <scope>provided</scope>
                                    </scopes>
                                    <findAllDuplicates>true</findAllDuplicates>
                                    <ignoreWhenIdentical>false</ignoreWhenIdentical>
                                </banDuplicateClasses>
                                <banCircularDependencies/>
                                <requireEncoding>
                                    <encoding>UTF-8</encoding>
                                    <includes>src/main/java/**,src/test/java/**</includes>
                                </requireEncoding>
                            </rules>

                            <!-- Doing a build for every error is a mess -->
                            <failFast>false</failFast>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <!-- It slows down the entire build
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>versions-maven-plugin</artifactId>
                <version>2.7</version>
                <executions>
                    <execution>
                        <phase>validate</phase>
                        <goals>
                            <goal>display-dependency-updates</goal>
                            <goal>display-plugin-updates</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <outputEncoding>UTF-8</outputEncoding>
                </configuration>
            </plugin> -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-clean-plugin</artifactId>
                <version>3.1.0</version>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-install-plugin</artifactId>
                <version>3.0.0-M1</version>
            </plugin>
            <plugin>
                <groupId>org.jetbrains.kotlin</groupId>
                <artifactId>kotlin-maven-plugin</artifactId>
                <version>1.4.10</version>
                <executions>
                    <execution>
                        <id>compile</id>
                        <phase>process-sources</phase>
                        <goals>
                            <goal>compile</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>test-compile</id>
                        <phase>process-test-sources</phase>
                        <goals>
                            <goal>test-compile</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <args>
                        <arg>-Xjsr305=warn</arg>
                        <arg>-Xjvm-default=enable</arg>
                        <!-- It creates a runtime overhead,
                            we already have compile time null-safety. -->
                        <arg>-Xno-param-assertions</arg>
                        <arg>-Xno-call-assertions</arg>
                        <arg>-Xno-receiver-assertions</arg>

                        <arg>-Xassertions=jvm</arg>

                        <arg>-Xskip-runtime-version-check</arg>
                        <arg>-Xsuppress-missing-builtins-error</arg>

                        <arg>-Xno-exception-on-explicit-equals-for-boxed-null</arg>

                        <arg>-Xallow-no-source-files</arg>
                    </args>
                    <!-- Target and arguments same as with javac. -->
                    <jvmTarget>1.8</jvmTarget>
                    <javacOptions>
                        <javacOption>-encoding</javacOption>
                        <javacOption>UTF-8</javacOption>

                        <javacOption>-O</javacOption>

                        <javacOption>-source</javacOption>
                        <javacOption>8</javacOption>

                        <javacOption>-target</javacOption>
                        <javacOption>8</javacOption>
                    </javacOptions>
                    <!-- Strictly define language and api version. -->
                    <languageVersion>1.4</languageVersion>
                    <apiVersion>1.4</apiVersion>
                    <!-- Hacky way to fix build warning in tests. -->
                    <annotationProcessors/>
                    <annotationProcessorPaths/>
                    <annotationProcessorArgs/>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.8.1</version>
                <configuration>
                    <skipMain>false</skipMain>
                    <skip>false</skip>
                    <source>8</source>
                    <target>8</target>
                    <fork>true</fork>
                    <optimize>true</optimize>
                    <forceJavacCompilerUse>false</forceJavacCompilerUse>
                    <compilerId>eclipse</compilerId>
                    <compilerVersion>14</compilerVersion>
                    <compilerArgs>
                        <arg>-properties</arg>
                        <arg>.settings/org.eclipse.jdt.core.prefs</arg>

                        <arg>-encoding</arg>
                        <arg>UTF-8</arg>

                        <arg>-O</arg>

                        <arg>-Xpkginfo:always</arg>
                    </compilerArgs>
                    <showWarnings>true</showWarnings>
                    <showDeprecation>true</showDeprecation>

                    <!-- Bugged option, false means incremental compilation is enabled -->
                    <!--<useIncrementalCompilation>false</useIncrementalCompilation>-->
                </configuration>
                <dependencies>
                    <dependency>
                        <groupId>org.codehaus.plexus</groupId>
                        <artifactId>plexus-compiler-eclipse</artifactId>
                        <version>2.8.8</version>
                        <exclusions>
                            <exclusion>
                                <groupId>org.codehaus.plexus</groupId>
                                <artifactId>plexus-utils</artifactId>
                            </exclusion>
                            <exclusion>
                                <groupId>org.eclipse.jdt</groupId>
                                <artifactId>ecj</artifactId>
                            </exclusion>
                        </exclusions>
                    </dependency>
                    <dependency>
                        <groupId>org.codehaus.plexus</groupId>
                        <artifactId>plexus-utils</artifactId>
                        <version>3.3.0</version>
                    </dependency>
                    <!-- This not a downgrade, eclipse version system is shitty,
                            this actually the 4.x version. -->
                    <dependency>
                        <groupId>org.eclipse.jdt</groupId>
                        <artifactId>ecj</artifactId>
                        <version>3.23.0</version>
                    </dependency>
                </dependencies>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>build-helper-maven-plugin</artifactId>
                <version>3.2.0</version>
                <executions>
                    <execution>
                        <id>regex-property</id>
                        <phase>validate</phase>
                        <goals>
                            <goal>regex-property</goal>
                        </goals>
                        <configuration>
                            <name>project.internalPackage</name>
                            <value>${project.version}</value>

                            <regex>(\d+)\.(\d+)(?:\.(\d+))?\s*(.*)</regex>

                            <replacement>ch.njol.internal.skript.v$1_$3_R$2</replacement>
                            <failIfNoMatch>true</failIfNoMatch>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-shade-plugin</artifactId>
                <version>3.2.4</version>
                <executions>
                    <execution>
                        <id>shade</id>
                        <phase>package</phase>
                        <goals>
                            <goal>shade</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <createDependencyReducedPom>false</createDependencyReducedPom>
                    <minimizeJar>false</minimizeJar>
                    <artifactSet>
                        <includes>
                            <include>*:*</include>
                        </includes>
                    </artifactSet>
                    <filters>
                        <filter>
                            <artifact>*:*</artifact>
                            <excludes>
                                <!-- Eclipse annotations includes bunch of un required stuff -->
                                <exclude>META-INF/*.SF</exclude>
                                <exclude>META-INF/*.DSA</exclude>
                                <exclude>META-INF/*.RSA</exclude>

                                <exclude>bundle.properties</exclude>
                                <exclude>src/</exclude>
                                <exclude>about.html</exclude>

                                <exclude>**/*.html</exclude>
                                <exclude>.api_description</exclude>

                                <!-- Unrequired kotlin files - we already remove some of them with proguard. -->
                                <exclude>**/*.kotlin_metadata</exclude>
                                <exclude>**/*.kotlin_builtins</exclude>
                                <exclude>**/*.kotlin_module</exclude>

                                <exclude>META-INF/Skript.kotlin_module</exclude>
                                <exclude>META-INF/LICENSE.txt</exclude>

                                <exclude>META-INF/maven/com.google.code.findbugs/</exclude>
                                <exclude>META-INF/maven/javax.annotation/</exclude>

                                <exclude>META-INF/kotlin-stdlib.kotlin_module</exclude>

                                <exclude>META-INF/kotlin-stdlib_coroutinesExperimental.kotlin_module</exclude>
                                <exclude>META-INF/kotlin-stdlib-coroutines.kotlin_module</exclude>
