    <build>
        <sourceDirectory>src/main/java</sourceDirectory>
        <testSourceDirectory>src/test/java</testSourceDirectory>

        <finalName>${project.artifactId}</finalName>
        <resources>
            <resource>
                <directory>src/main/resources/</directory>
                <filtering>true</filtering>
                <includes>
                    <include>plugin.yml</include>
                    <include>version</include>
                    <include>META-INF/*</include>
                    <include>config.sk</include>
                    <include>lang/*</include>
                </includes>
            </resource>
            <resource>
                <directory>src/main/resources/</directory>
                <filtering>false</filtering>
                <includes>
                    <include>**/*</include>
                </includes>
                <excludes>
                    <exclude>plugin.yml</exclude>
                    <exclude>version</exclude>
                    <exclude>META-INF/*</exclude>
                    <exclude>config.sk</exclude>
                    <exclude>lang/*</exclude>
                </excludes>
            </resource>
            <resource>
                <directory>./</directory>
                <filtering>false</filtering>
                <includes>
                    <include>LICENSE</include>
                    <include>README.md</include>
                </includes>
            </resource>
        </resources>

        <testResources>
            <testResource>
                <directory>src/test/resources/</directory>
                <filtering>false</filtering>
            </testResource>
        </testResources>

        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-enforcer-plugin</artifactId>
                <version>3.0.0-M2</version>
                <executions>
                    <execution>
                        <id>enforce-maven</id>
                        <phase>validate</phase>
                        <goals>
                            <goal>enforce</goal>
                        </goals>
                        <configuration>
                            <rules>
                                <requireMavenVersion>
                                    <version>3.0.5</version>
                                </requireMavenVersion>
                            </rules>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
            <!-- It slows down the entire build
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>versions-maven-plugin</artifactId>
                <version>2.7</version>
                <executions>
                    <execution>
                        <phase>validate</phase>
                        <goals>
                            <goal>display-dependency-updates</goal>
                            <goal>display-plugin-updates</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <outputEncoding>UTF-8</outputEncoding>
                </configuration>
            </plugin> -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-clean-plugin</artifactId>
                <version>3.1.0</version>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-install-plugin</artifactId>
                <version>3.0.0-M1</version>
            </plugin>
            <plugin>
                <groupId>org.jetbrains.kotlin</groupId>
                <artifactId>kotlin-maven-plugin</artifactId>
                <version>1.3.40</version>
                <executions>
                    <execution>
                        <id>compile</id>
                        <phase>process-sources</phase>
                        <goals>
                            <goal>compile</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>test-compile</id>
                        <phase>process-test-sources</phase>
                        <goals>
                            <goal>test-compile</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <args>
                        <arg>-Xjsr305=ignore</arg>
                        <arg>-Xjvm-default=compatibility</arg>
                        <!-- It creates a runtime overhead,
                            we already have compile time null-safety. -->
                        <arg>-Xno-param-assertions</arg>
                        <arg>-Xno-call-assertions</arg>
                        <arg>-Xno-receiver-assertions</arg>

                        <arg>-Xassertions=jvm</arg>

                        <arg>-Xskip-runtime-version-check</arg>
                        <!--<arg>-Xsuppress-missing-builtins-error</arg>-->

                        <arg>-Xno-exception-on-explicit-equals-for-boxed-null</arg>
                    </args>
                    <!-- Target and arguments same as with javac. -->
                    <jvmTarget>1.8</jvmTarget>
                    <javacOptions>
                        <javacOption>-O</javacOption>
                    </javacOptions>
                    <!-- Strictly define language and api version. -->
                    <languageVersion>1.3</languageVersion>
                    <apiVersion>1.3</apiVersion>
                    <!-- Hacky way to fix build warning in tests. -->
                    <annotationProcessors/>
                    <annotationProcessorPaths/>
                    <annotationProcessorArgs/>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.8.1</version>
                <configuration>
                    <skipMain>false</skipMain>
                    <skip>false</skip>
                    <source>1.8</source>
                    <target>1.8</target>
                    <optimize>true</optimize>
                    <fork>true</fork>
                    <forceJavacCompilerUse>false</forceJavacCompilerUse>
                    <compilerId>eclipse</compilerId>
                    <compilerVersion>1.8</compilerVersion>
                    <compilerArgs>
                        <arg>-properties</arg>
                        <arg>.settings/org.eclipse.jdt.core.prefs</arg>

                        <arg>-encoding</arg>
                        <arg>UTF-8</arg>

                        <arg>-O</arg>
                    </compilerArgs>
                </configuration>
                <dependencies>
                    <dependency>
                        <groupId>org.codehaus.plexus</groupId>
                        <artifactId>plexus-compiler-eclipse</artifactId>
                        <version>2.8.5</version>
                    </dependency>
                    <dependency>
                        <groupId>org.eclipse.jdt.core.compiler</groupId>
                        <artifactId>ecj</artifactId>
                        <version>4.12RC1a</version>
                    </dependency>
                </dependencies>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-shade-plugin</artifactId>
                <version>3.2.1</version>
                <executions>
                    <execution>
                        <id>shade</id>
                        <phase>package</phase>
                        <goals>
                            <goal>shade</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <createDependencyReducedPom>false</createDependencyReducedPom>
                    <artifactSet>
                        <includes>
                            <include>*:*</include>
                        </includes>
                    </artifactSet>
                    <filters>
                        <filter>
                            <artifact>*:*</artifact>
                            <excludes>
                                <!-- Eclipse annotations includes bunch of un required stuff -->
                                <exclude>META-INF/*.SF</exclude>
                                <exclude>META-INF/*.DSA</exclude>
                                <exclude>META-INF/*.RSA</exclude>

                                <exclude>bundle.properties</exclude>
                                <exclude>src/</exclude>
                                <exclude>about.html</exclude>
                                <exclude>**/*.html</exclude>
                                <exclude>.api_description</exclude>

                                <!-- Unrequired kotlin files - we already remove some of them with proguard. -->
                                <exclude>**/*.kotlin_metadata</exclude>
                                <exclude>**/*.kotlin_builtins</exclude>

                                <exclude>META-INF/Skript.kotlin_module</exclude>
                                <exclude>META-INF/LICENSE.txt</exclude>

                                <exclude>META-INF/maven/com.google.code.findbugs/</exclude>
                                <exclude>META-INF/maven/javax.annotation/</exclude>

                                <exclude>META-INF/kotlin-stdlib.kotlin_module</exclude>
                                <exclude>META-INF/kotlin-stdlib_coroutinesExperimental.kotlin_module</exclude>
                                <exclude>META-INF/kotlin-stdlib-common.kotlin_module</exclude>
                                <exclude>META-INF/kotlin-stdlib-common-coroutines.kotlin_module</exclude>
                                <exclude>META-INF/kotlin-stdlib-jdk7.kotlin_module</exclude>
                                <exclude>META-INF/kotlin-stdlib-jdk8.kotlin_module</exclude>
                            </excludes>
                        </filter>
                    </filters>
                    <relocations>
                        <!-- Don't share the Kotlin library with other plugins
                                Also: Add-ons should not use these libraries. -->
                        <relocation>
                            <pattern>kotlin</pattern>
                            <shadedPattern>ch.njol.libraries.kotlin</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.jetbrains.annotations</pattern>
                            <shadedPattern>ch.njol.libraries.annotations.jetbrains</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.intellij.lang.annotations</pattern>
                            <shadedPattern>ch.njol.libraries.annotations.intellij</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>org.eclipse.jdt.annotation</pattern>
                            <shadedPattern>ch.njol.libraries.annotations.eclipse</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>javax.annotation</pattern>
                            <shadedPattern>ch.njol.libraries.annotations.javax</shadedPattern>
                        </relocation>
                        <relocation>
                            <pattern>edu.umd.cs.findbugs.annotations</pattern>
                            <shadedPattern>ch.njol.libraries.annotations.findbugs</shadedPattern>
                        </relocation>
                    </relocations>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <version>3.1.2</version>
                <configuration>
                    <archive>
                        <index>true</index>
                        <manifest>
                            <addClasspath>false</addClasspath>
                            <addDefaultImplementationEntries>true</addDefaultImplementationEntries>
                            <addDefaultSpecificationEntries>true</addDefaultSpecificationEntries>
                        </manifest>
                        <manifestEntries>
                            <Manifest-Version>1.0</Manifest-Version>
                            <Implementation-Title>Skript</Implementation-Title>
                            <Implementation-Version>${project.version}</Implementation-Version>
                            <Main-Class>ch.njol.skript.SkriptCLI</Main-Class>
                            <Sealed>true</Sealed>
                            <Built-By>Skript Team</Built-By>
                            <Automatic-Module-Name>ch.njol.skript</Automatic-Module-Name>
                            <Specification-Vendor>Skript Team</Specification-Vendor>
                            <Specification-Title>Skript</Specification-Title>
                            <Implementation-Vendor>Skript Team</Implementation-Vendor>
                            <Specification-Version>${project.version}</Specification-Version>

                            <Name>ch/njol</Name>
                            <Sealed>true</Sealed>
                        </manifestEntries>
                    </archive>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-resources-plugin</artifactId>
                <version>3.1.0</version>
                <configuration>
                    <encoding>UTF-8</encoding>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <version>3.0.0-M3</version>
                <configuration>
                    <useFile>false</useFile>
                    <argLine>-Dfile.encoding=UTF-8</argLine>
                    <trimStackTrace>false</trimStackTrace>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-deploy-plugin</artifactId>
                <version>3.0.0-M1</version>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-site-plugin</artifactId>
                <version>3.7.1</version>
            </plugin>
        </plugins>
    </build>

    <profiles>
        <!--This plugin's configuration is used to store Eclipse m2e settings
          only. It has no influence on the Maven build itself. -->
        <profile>
            <id>only-eclipse</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>m2e.version</name>
                </property>
            </activation>
            <build>
                <pluginManagement>
                    <plugins>
                        <plugin>
                            <groupId>org.eclipse.m2e</groupId>
                            <artifactId>lifecycle-mapping</artifactId>
                            <version>1.0.0</version>
                            <configuration>
                                <lifecycleMappingMetadata>
                                    <pluginExecutions>
                                        <pluginExecution>
                                            <pluginExecutionFilter>
                                                <groupId>
                                                    org.apache.maven.plugins
                                                </groupId>
                                                <artifactId>
                                                    maven-compiler-plugin
                                                </artifactId>
                                                <versionRange>
